CREATE MEMORY TABLE PUBLIC.USERS(USERNAME VARCHAR(20),PASSWORD VARCHAR(20), PRIMARY KEY (USERNAME))
CREATE MEMORY TABLE PUBLIC.HABITS(NAME VARCHAR(25), WEEKAMT INTEGER,COMPLETEAMT INTEGER, LASTCOMPLETEDATE VARCHAR(14), TIME VARCHAR(20), SORTTIME INTEGER, CREATEDATE VARCHAR(14), TOTALCOMPLETEAMT INTEGER, MON INTEGER, TUES INTEGER, WED INTEGER, THRU INTEGER, FRI INTEGER, SAT INTEGER, SUN INTEGER, USERNAME VARCHAR(20), PRIMARY KEY (NAME,USERNAME), FOREIGN KEY (USERNAME) REFERENCES USERS(USERNAME))
CREATE MEMORY TABLE PUBLIC.NOTES(NAME VARCHAR(25),USERNAME VARCHAR(20), TEXT VARCHAR(255), FEELING INTEGER, DATE VARCHAR(14), PRIMARY KEY (NAME, USERNAME, TEXT), FOREIGN KEY (NAME, USERNAME) REFERENCES HABITS(NAME, USERNAME) ON DELETE CASCADE ON UPDATE CASCADE)
