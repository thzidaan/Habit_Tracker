<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comp3350.habittracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Presentation.AddHabitActivity"></activity>
        <activity android:name=".Presentation.ViewHabitsActivity"></activity>
        <activity android:name=".Presentation.CreateNewNoteActivity"></activity>
        <activity android:name=".Presentation.ViewHabitStatsActivity"></activity>
        <activity android:name=".Presentation.RegisterActivity"></activity>
        <activity android:name=".Presentation.HomeActivity"></activity>
        <activity android:name=".Presentation.ChangePasswordActivity"></activity>
        <activity android:name=".Presentation.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>